import random

def gen_pass(pass_length):
    elements = "+-/*!&$#?=@<hufejsoi8239r7ualwpknpq>"
    password = ""
    for i in range(pass_length):
        password += random.choice(elements)
    return password

def ping():
    return ('pong')

def money():
    for i in range(2):
        g = random.randint(0,2)
        if g == 0:
            return ('cara')
        elif g == 1:
            return ('cruz')
        else:
            return ('empate')
